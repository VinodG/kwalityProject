<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/rlrlrl"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:background="#696969"
    android:orientation="vertical" >

    <LinearLayout
        android:id="@+id/llMenuBody"
        android:layout_width="wrap_content"
        android:layout_height="fill_parent"
        android:layout_marginRight="5dip"
        android:background="#1C1C1C"
        android:orientation="vertical"
        android:paddingTop="3dip"
        android:visibility="gone" >

        <!-- 1 -->

        <LinearLayout
            android:id="@+id/llStockLoading"
            android:layout_width="fill_parent"
            android:layout_height="0dp"
            android:layout_margin="2dip"
            android:layout_weight="1"
            android:gravity="center_vertical"
            android:orientation="horizontal" >

            <ImageView
                android:layout_width="30dip"
                android:layout_height="30dip"
                android:background="@drawable/stockloading" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="10dip"
                android:text="Stock Loading"
                android:textColor="@color/white"
                android:textSize="14dip" />
        </LinearLayout>

        <ImageView
            android:layout_width="fill_parent"
            android:layout_height="1dp"
            android:background="@color/black" />

        <!-- 2 -->

        <LinearLayout
            android:id="@+id/llAdvanceorderBoking"
            android:layout_width="fill_parent"
            android:layout_height="0dp"
            android:layout_margin="2dip"
            android:layout_weight="1"
            android:gravity="center_vertical"
            android:orientation="horizontal" >

            <ImageView
                android:layout_width="30dip"
                android:layout_height="30dip"
                android:background="@drawable/advanceorderboking" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="10dip"
                android:text="Advance Order Booking"
                android:textColor="@color/white"
                android:textSize="14dip" />
        </LinearLayout>

        <ImageView
            android:layout_width="fill_parent"
            android:layout_height="1dp"
            android:background="@color/black" />

        <!-- 3 -->

        <LinearLayout
            android:id="@+id/llOrderDelivered"
            android:layout_width="fill_parent"
            android:layout_height="0dp"
            android:layout_margin="2dip"
            android:layout_weight="1"
            android:gravity="center_vertical"
            android:orientation="horizontal" >

            <ImageView
                android:layout_width="30dip"
                android:layout_height="30dip"
                android:background="@drawable/orderdelivered" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="10dip"
                android:text="Order History"
                android:textColor="@color/white"
                android:textSize="14dip" />
        </LinearLayout>

        <ImageView
            android:layout_width="fill_parent"
            android:layout_height="1dp"
            android:background="@color/black" />
        <!-- 4 -->

        <LinearLayout
            android:id="@+id/llPaymentReceived"
            android:layout_width="fill_parent"
            android:layout_height="0dp"
            android:layout_margin="2dip"
            android:layout_weight="1"
            android:gravity="center_vertical"
            android:orientation="horizontal" >

            <ImageView
                android:layout_width="30dip"
                android:layout_height="30dip"
                android:background="@drawable/a4" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="10dip"
                android:text="Payment Recieved"
                android:textColor="@color/white"
                android:textSize="14dip" />
        </LinearLayout>

        <ImageView
            android:layout_width="fill_parent"
            android:layout_height="1dp"
            android:background="@color/black" />

        <!-- 5 -->

        <LinearLayout
            android:id="@+id/llOrderPlaced"
            android:layout_width="fill_parent"
            android:layout_height="0dp"
            android:layout_margin="2dip"
            android:layout_weight="1"
            android:gravity="center_vertical"
            android:orientation="horizontal" >

            <ImageView
                android:layout_width="30dip"
                android:layout_height="30dip"
                android:background="@drawable/a5" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="10dip"
                android:text="Order Placed"
                android:textColor="@color/white"
                android:textSize="14dip" />
        </LinearLayout>

        <ImageView
            android:layout_width="fill_parent"
            android:layout_height="1dp"
            android:background="@color/black" />

        <!-- 6 -->

        <LinearLayout
            android:id="@+id/llReturnsOrder"
            android:layout_width="fill_parent"
            android:layout_height="0dp"
            android:layout_margin="2dip"
            android:layout_weight="1"
            android:gravity="center_vertical"
            android:orientation="horizontal" >

            <ImageView
                android:layout_width="30dip"
                android:layout_height="30dip"
                android:background="@drawable/a6" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="10dip"
                android:text="Returns Order"
                android:textColor="@color/white"
                android:textSize="14dip" />
        </LinearLayout>

        <ImageView
            android:layout_width="fill_parent"
            android:layout_height="1dp"
            android:background="@color/black" />
        <!-- 7 -->

        <LinearLayout
            android:id="@+id/llStockReports"
            android:layout_width="fill_parent"
            android:layout_height="0dp"
            android:layout_margin="2dip"
            android:layout_weight="1"
            android:gravity="center_vertical"
            android:orientation="horizontal" >

            <ImageView
                android:layout_width="30dip"
                android:layout_height="30dip"
                android:background="@drawable/a7" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="10dip"
                android:text="Stock Reports"
                android:textColor="@color/white"
                android:textSize="14dip" />
        </LinearLayout>

        <ImageView
            android:layout_width="fill_parent"
            android:layout_height="1dp"
            android:background="@color/black" />

        <!-- 8 -->

        <LinearLayout
            android:id="@+id/llTransfers"
            android:layout_width="fill_parent"
            android:layout_height="0dp"
            android:layout_margin="2dip"
            android:layout_weight="1"
            android:gravity="center_vertical"
            android:orientation="horizontal" >

            <ImageView
                android:layout_width="30dip"
                android:layout_height="30dip"
                android:background="@drawable/a8" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="10dip"
                android:text="Transfer In/Transfer Out"
                android:textColor="@color/white"
                android:textSize="14dip" />
        </LinearLayout>

        <ImageView
            android:layout_width="fill_parent"
            android:layout_height="1dp"
            android:background="@color/black" />

        <!-- 9 -->

        <LinearLayout
            android:id="@+id/llDirectInvoicing"
            android:layout_width="fill_parent"
            android:layout_height="0dp"
            android:layout_margin="2dip"
            android:layout_weight="1"
            android:gravity="center_vertical"
            android:orientation="horizontal" >

            <ImageView
                android:layout_width="30dip"
                android:layout_height="30dip"
                android:background="@drawable/a9" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="10dip"
                android:text="Direct Invoicing"
                android:textColor="@color/white"
                android:textSize="14dip" />
        </LinearLayout>

        <ImageView
            android:layout_width="fill_parent"
            android:layout_height="1dp"
            android:background="@color/black" />
        <!-- 10 -->

        <LinearLayout
            android:id="@+id/llReturnStock"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:layout_margin="2dip"
            android:gravity="center_vertical"
            android:orientation="horizontal" >

            <ImageView
                android:layout_width="30dip"
                android:layout_height="30dip"
                android:background="@drawable/a10" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="10dip"
                android:text="Return Stock"
                android:textColor="@color/white"
                android:textSize="14dip" />
        </LinearLayout>

        <ImageView
            android:layout_width="fill_parent"
            android:layout_height="1dp"
            android:background="@color/black" />

        <!-- 11 -->

        <LinearLayout
            android:id="@+id/llEmptyJarReturns"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:layout_margin="2dip"
            android:gravity="center_vertical"
            android:orientation="horizontal" >

            <ImageView
                android:layout_width="30dip"
                android:layout_height="30dip"
                android:background="@drawable/a11" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="10dip"
                android:text="Empty Jar Returns"
                android:textColor="@color/white"
                android:textSize="14dip" />
        </LinearLayout>

        <ImageView
            android:layout_width="fill_parent"
            android:layout_height="1dp"
            android:background="@color/black" />

        <!-- 12 -->

        <LinearLayout
            android:id="@+id/llFreeDelivery"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:layout_margin="2dip"
            android:gravity="center_vertical"
            android:orientation="horizontal" >

            <ImageView
                android:layout_width="30dip"
                android:layout_height="30dip"
                android:background="@drawable/a12" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="10dip"
                android:text="Free Delivery"
                android:textColor="@color/white"
                android:textSize="14dip" />
        </LinearLayout>

        <ImageView
            android:layout_width="fill_parent"
            android:layout_height="1dp"
            android:background="@color/black" />
        <!-- 13 -->

        <LinearLayout
            android:id="@+id/llLogout"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:layout_margin="2dip"
            android:gravity="center_vertical"
            android:orientation="horizontal" >

            <ImageView
                android:layout_width="30dip"
                android:layout_height="30dip"
                android:background="@drawable/a13" />

            <TextView
                android:id="@+id/tvBaseSystemParameters"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="10dip"
                android:text="Logout"
                android:textColor="@color/white"
                android:textSize="14dip" />
        </LinearLayout>

        <ImageView
            android:layout_width="fill_parent"
            android:layout_height="1dp"
            android:background="@color/black" />
    </LinearLayout>

    <LinearLayout
        android:id="@+id/llApp"
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:background="#E9E9E9"
        android:orientation="vertical"
        android:visibility="visible" >

        <LinearLayout
            android:id="@+id/llTopBase"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@drawable/topnav"
            android:orientation="horizontal" >

            <ImageView
                android:id="@+id/iv_menu"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:layout_marginLeft="15dp"
                android:background="@drawable/menu"
                android:contentDescription="@string/app_name" />

            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:layout_marginBottom="2dp"
                android:layout_marginLeft="10dp"
                android:background="@drawable/toplogo2"
                android:contentDescription="@string/app_name" />

            <View
                android:layout_width="0dp"
                android:layout_height="2dp"
                android:layout_weight="1" />

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="vertical" >

                <TextView
                    android:id="@+id/tv_PresellerName"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="10dp"
                    android:layout_marginTop="10dp"
                    android:text="Abhinav Kumar"
                    android:textColor="#03577C"
                    android:textSize="20sp" />

                <TextView
                    android:id="@+id/tv_PresellerCatagory"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="right"
                    android:layout_marginBottom="10dp"
                    android:layout_marginLeft="15dp"
                    android:text="SALESMAN"
                    android:textColor="#4F90D4"
                    android:textSize="16sp" />
            </LinearLayout>

            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:layout_margin="5dp"
                android:background="@drawable/topsaparetor"
                android:contentDescription="@string/app_name" />

            <ImageView
                android:id="@+id/iv_Logout"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:layout_marginRight="10dp"
                android:background="@drawable/logout"
                android:contentDescription="@string/app_name" />
        </LinearLayout>

        <LinearLayout
            android:id="@+id/llMiddleBase"
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_weight="1"
            android:orientation="vertical" >
        </LinearLayout>

    </LinearLayout>

</FrameLayout>