<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:orientation="vertical" >

    <LinearLayout
        android:id="@+id/llTotalLayout"
        android:layout_width="fill_parent"
        android:layout_height="40dip"
        android:background="@drawable/back"
        android:gravity="center_vertical"
        android:orientation="vertical"
        android:visibility="gone" >

        <LinearLayout
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:layout_margin="5dip"
            android:gravity="center_vertical"
            android:orientation="horizontal"
            android:visibility="gone" >

            <TextView
                android:id="@+id/tvDeliveryDiscount"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="15dip"
                android:layout_weight="1"
                android:singleLine="true"
                android:text="Discount_if_any_inpercentage"
                android:textColor="@color/gray_light"
                android:textSize="@dimen/text_size_normal" />

            <EditText
                android:id="@+id/edtDeliverydiscount"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginRight="10dip"
                android:background="@drawable/smalltextinputbg"
                android:gravity="center"
                android:imeOptions="actionNext"
                android:inputType="number"
                android:text="0"
                android:textColor="@color/gray_light"
                android:textSize="@dimen/text_size_normal" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="5dip"
            android:layout_marginRight="5dip"
            android:gravity="center_vertical"
            android:orientation="horizontal" >

            <TextView
                android:id="@+id/tvDeliveryAmount"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="15dip"
                android:layout_weight="1"
                android:singleLine="true"
                android:text="Total Amount (AED)"
                android:textColor="@color/gray_light"
                android:textSize="@dimen/text_size_normal" />

            <EditText
                android:id="@+id/edtDeliveryTotalamt"
                android:layout_width="wrap_content"
                android:layout_height="25dip"
                android:layout_marginRight="10dip"
                android:background="@drawable/smalltextinputbg"
                android:editable="false"
                android:enabled="false"
                android:focusable="false"
                android:gravity="center"
                android:imeOptions="actionDone"
                android:inputType="number"
                android:text="0"
                android:textColor="@color/gray_light"
                android:textSize="@dimen/text_size_normal" />
        </LinearLayout>
    </LinearLayout>

    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:layout_weight="1"
        android:orientation="vertical">

        <LinearLayout
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:background="@drawable/topbgbg"
            android:orientation="horizontal"
            android:visibility="gone">

            <ProgressBar
                android:id="@+id/ivProgressBar"
                android:layout_width="30dip"
                android:layout_height="30dip"
                android:layout_gravity="center_vertical" />

            <TextView
                android:id="@+id/tvTransactionType"
                style="@style/TextHeaderLargeW0"
                android:layout_width="wrap_content"
                android:gravity="center_vertical" />
        </LinearLayout>

        <include
            android:id="@+id/inHeader"
            layout="@layout/order_summary_top_bar" />

        <include
            android:id="@+id/inDivider"
            layout="@layout/divider_dotted"
            android:visibility="gone" />

        <LinearLayout
            android:id="@+id/llBottom"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:gravity="center_vertical|right"
            android:padding="@dimen/margin_5">

            <TextView
                android:id="@+id/tvPageTitle"
                style="@style/TextHeaderGrayW0"
                android:layout_width="fill_parent"
                android:layout_weight="1"
                android:gravity="center_vertical" />

            <Button
                android:id="@+id/btnOrderedQuantity"
                style="@style/Btn_Green"
                android:drawableLeft="@drawable/icon5"
                android:text="Ordered Quantity "
                android:visibility="gone" />

            <Button
                android:id="@+id/btnInventory"
                style="@style/Btn_Green"
                android:drawableLeft="@drawable/icon5"
                android:text="Inventory "
                android:visibility="gone" />

            <Button
                android:id="@+id/btnReturnQT"
                style="@style/Btn_Green"
                android:drawableLeft="@drawable/icon5"
                android:text="Return Order Quantity "
                android:visibility="gone" />

            <Button
                android:id="@+id/btnUndeliveredQT"
                style="@style/Btn_Green"
                android:drawableLeft="@drawable/icon5"
                android:drawablePadding="5dip"
                android:text="Undelivered Quantity"
                android:visibility="gone" />

            <Button
                android:id="@+id/btnDeliveredQuantity"
                style="@style/Btn_Green"
                android:drawableLeft="@drawable/icon5"
                android:text="Delivered Quantity "
                android:visibility="gone" />

            <TextView
                android:id="@+id/tvPendingImage"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text=""
                android:textColor="@color/red_dark" />

            <Button
                android:id="@+id/btnOK"
                style="@style/Btn_Green"
                android:drawableLeft="@drawable/check5"
                android:text="OK"
                android:visibility="visible" />

            <Button
                android:id="@+id/btnStockUnload"
                style="@style/Btn_Green"
                android:drawableLeft="@drawable/stock_nuload"
                android:text="Stock Unload"
                android:visibility="gone" />

            <Button
                android:id="@+id/btnUpload"
                style="@style/Btn_Green"
                android:drawableLeft="@drawable/tick"
                android:text="Submit EOT" />

            <Button
                android:id="@+id/btnDeliveredContinue"
                style="@style/Btn_Green_wrap"
                android:drawableLeft="@drawable/cont"
                android:text="Continue"
                android:visibility="gone" />
        </LinearLayout>

        <include layout="@layout/divider_dotted" />

        <LinearLayout
            android:id="@+id/llTypeList"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">

            <TextView
                android:id="@+id/tvOrderReceived"
                android:layout_width="fill_parent"
                android:layout_height="@dimen/settings_cell_height"
                android:drawableLeft="@drawable/order_summary_icon"
                android:drawablePadding="10dp"
                android:drawableRight="@drawable/next_arrow"
                android:gravity="center_vertical"
                android:paddingLeft="10dip"
                android:paddingRight="10dip"
                android:text="Order Received"
                android:textColor="@color/black"
                android:textSize="@dimen/text_size_extra_large" />

            <include layout="@layout/divider_normal" />

            <LinearLayout
                android:id="@+id/llOrderReceived"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:visibility="gone"></LinearLayout>

            <TextView
                android:id="@+id/tvReturnRequest"
                android:layout_width="fill_parent"
                android:layout_height="@dimen/settings_cell_height"
                android:drawableLeft="@drawable/stock_inventory_icon"
                android:drawablePadding="10dp"
                android:drawableRight="@drawable/next_arrow"
                android:gravity="center_vertical"
                android:paddingLeft="10dip"
                android:paddingRight="10dip"
                android:text="Goods_Return_Voucher"
                android:textColor="@color/black"
                android:textSize="@dimen/text_size_large"
                android:visibility="gone" />

            <LinearLayout
                android:id="@+id/llReturns"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:visibility="gone"></LinearLayout>

            <TextView
                android:id="@+id/tvAmountCollected"
                android:layout_width="fill_parent"
                android:layout_height="@dimen/settings_cell_height"
                android:drawableLeft="@drawable/payment_summary_icon"
                android:drawablePadding="10dp"
                android:drawableRight="@drawable/next_arrow"
                android:gravity="center_vertical"
                android:paddingLeft="10dip"
                android:paddingRight="10dip"
                android:text="Payment Summary"
                android:textColor="@color/black"
                android:textSize="@dimen/text_size_extra_large" />

            <include layout="@layout/divider_normal" />

            <TextView
                android:id="@+id/tvUnUploadedData"
                android:layout_width="fill_parent"
                android:layout_height="50dip"
                android:background="@drawable/list_item_selected"
                android:drawableRight="@drawable/next_arrow"
                android:gravity="center_vertical"
                android:paddingLeft="10dip"
                android:paddingRight="10dip"
                android:text="Upload Status"
                android:textColor="@color/black"
                android:textSize="@dimen/text_size_normal"
                android:visibility="gone" />

            <TextView
                android:id="@+id/tvUndeliveredQT"
                android:layout_width="fill_parent"
                android:layout_height="@dimen/settings_cell_height"
                android:drawableLeft="@drawable/stock_inventory_icon"
                android:drawablePadding="10dp"
                android:drawableRight="@drawable/next_arrow"
                android:gravity="center_vertical"
                android:paddingLeft="10dip"
                android:paddingRight="10dip"
                android:text="Stock Inventory"
                android:textColor="@color/black"
                android:textSize="@dimen/text_size_extra_large" />

            <include layout="@layout/divider_normal" />

            <TextView
                android:id="@+id/tvUnvisitedcustomer"
                android:layout_width="fill_parent"
                android:layout_height="@dimen/settings_cell_height"
                android:drawableLeft="@drawable/stock_inventory_icon"
                android:drawablePadding="10dp"
                android:drawableRight="@drawable/next_arrow"
                android:gravity="center_vertical"
                android:paddingLeft="10dip"
                android:paddingRight="10dip"
                android:text="Un Visited Customer"
                android:textColor="@color/black"
                android:textSize="@dimen/text_size_extra_large"
                android:visibility="visible" />

            <include layout="@layout/divider_normal" />

            <TextView
                android:id="@+id/tvReturnOrderQT"
                android:layout_width="fill_parent"
                android:layout_height="50dip"
                android:background="@drawable/list_item_selected"
                android:drawableRight="@drawable/next_arrow"
                android:gravity="center_vertical"
                android:paddingLeft="10dip"
                android:paddingRight="10dip"
                android:text="Return Order Inventory"
                android:textColor="@color/black"
                android:textSize="@dimen/text_size_normal"
                android:visibility="gone" />

            <TextView
                android:id="@+id/tvReplacement"
                android:layout_width="fill_parent"
                android:layout_height="50dip"
                android:background="@drawable/list_item_selected"
                android:drawableRight="@drawable/next_arrow"
                android:gravity="center_vertical"
                android:paddingLeft="10dip"
                android:paddingRight="10dip"
                android:text="Replacement Order Inventory"
                android:textColor="@color/black"
                android:textSize="@dimen/text_size_normal"
                android:visibility="gone" />

            <LinearLayout
                android:id="@+id/llAmount"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:visibility="gone">

                <ImageView
                    android:id="@+id/ivLuluReturnHistory"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:background="@drawable/add" />

                <ImageView
                    android:id="@+id/ivCarefourReturnHistory"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:background="@drawable/add" />

                <ImageView
                    android:id="@+id/ivspinesReturnHistory"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:background="@drawable/add" />

                <ImageView
                    android:id="@+id/ivGeantReturnHistory"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:background="@drawable/add" />
            </LinearLayout>
        </LinearLayout>

        <LinearLayout
            android:id="@+id/llOrderDetailLayout"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:visibility="gone">

            <LinearLayout
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:padding="10dip">

                <LinearLayout
                    android:layout_width="0dip"
                    android:layout_height="fill_parent"
                    android:layout_marginRight="5dip"
                    android:layout_marginTop="5dip"
                    android:layout_weight="1"
                    android:gravity="center_vertical"
                    android:orientation="vertical">

                    <LinearLayout
                        android:id="@+id/llInvoiceLayout"
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content"
                        android:gravity="center_vertical"
                        android:visibility="gone">

                        <TextView
                            android:id="@+id/tvInvoiceNoTitle"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="Invoice_Number"
                            android:textColor="@color/gray_dark"
                            android:textStyle="bold" />

                        <TextView
                            android:id="@+id/tvInvoiceNoVal"
                            android:layout_width="fill_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="5dip"
                            android:singleLine="true"
                            android:text="200"
                            android:textColor="@color/gray_dark"
                            android:textSize="@dimen/text_size_normal"
                            android:textStyle="bold" />
                    </LinearLayout>

                    <LinearLayout
                        android:id="@+id/llLPONumber"
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content"
                        android:gravity="center_vertical"
                        android:visibility="gone">

                        <TextView
                            android:id="@+id/tvLpoNumer"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="LPO_Number"
                            android:textColor="@color/gray_dark"
                            android:textSize="@dimen/text_size_normal"
                            android:textStyle="bold" />

                        <TextView
                            android:id="@+id/tvLpoNumerval"
                            android:layout_width="fill_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="5dip"
                            android:singleLine="true"
                            android:textColor="@color/gray_dark"
                            android:textSize="@dimen/text_size_normal"
                            android:textStyle="bold" />
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content"
                        android:gravity="center_vertical">

                        <TextView
                            android:id="@+id/tvOrderNoTitle"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="Order No:"
                            android:textColor="@color/gray_dark"
                            android:textSize="@dimen/text_size_normal"
                            android:textStyle="bold" />

                        <TextView
                            android:id="@+id/tvOrderNoVal"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="5dip"
                            android:singleLine="true"
                            android:text="200"
                            android:textColor="@color/gray_dark"
                            android:textSize="@dimen/text_size_normal"
                            android:textStyle="bold" />

                        <TextView
                            android:id="@+id/tvOrderStatusTitle"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="7dip"
                            android:text="Status:"
                            android:textColor="@color/gray_dark"
                            android:textSize="@dimen/text_size_normal"
                            android:textStyle="bold" />

                        <TextView
                            android:id="@+id/tvOrderStatusVal"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="5dip"
                            android:singleLine="true"
                            android:text="Status_Order_Received"
                            android:textColor="@color/gray_dark"
                            android:textSize="@dimen/text_size_normal"
                            android:textStyle="bold" />
                    </LinearLayout>

                    <TextView
                        android:id="@+id/tvCustomerName"
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="3dip"
                        android:singleLine="true"
                        android:text="Carrefour"
                        android:textColor="@color/black"
                        android:textSize="@dimen/text_size_normal" />

                    <TextView
                        android:id="@+id/tvCustomerLocation"
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="2dip"
                        android:singleLine="true"
                        android:text="Downtown_Dubai"
                        android:textColor="@color/black"
                        android:textSize="@dimen/text_size_normal" />

                    <TextView
                        android:id="@+id/tvOrderType"
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="2dip"
                        android:singleLine="true"
                        android:text="Downtown_Dubai"
                        android:textColor="@color/black"
                        android:textSize="@dimen/text_size_normal"
                        android:visibility="gone" />

                    <TextView
                        android:id="@+id/tvErrorMessage"
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="2dip"
                        android:singleLine="true"
                        android:textColor="@color/black"
                        android:textSize="@dimen/text_size_normal"
                        android:visibility="gone" />
                </LinearLayout>
            </LinearLayout>
        </LinearLayout>

        <LinearLayout
            android:id="@+id/llCodeLayout"
            style="@style/Layout_Top_GreenBar"
            android:visibility="gone">

            <TextView
                android:id="@+id/tvCodeEOP"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="10dip"
                android:layout_weight="1"
                android:orientation="vertical"
                android:text="Item Code"
                android:textColor="@color/white"
                android:textSize="@dimen/text_size_normal"></TextView>

            <TextView
                android:id="@+id/tvCasesEOP"
                android:layout_width="70dip"
                android:layout_height="wrap_content"
                android:drawableLeft="@drawable/sapare"
                android:gravity="center"
                android:text="Cases"
                android:textColor="@color/white"
                android:textSize="@dimen/text_size_normal" />

            <TextView
                android:id="@+id/tvUnitsEOP"
                android:layout_width="71dip"
                android:layout_height="wrap_content"
                android:drawableLeft="@drawable/sapare"
                android:gravity="center"
                android:text="Units"
                android:textColor="@color/white"
                android:textSize="@dimen/text_size_normal" />
        </LinearLayout>

        <TextView
            android:id="@+id/tvNoOrderFound"
            android:layout_width="fill_parent"
            android:layout_height="fill_parent"
            android:layout_weight="1"
            android:gravity="center"
            android:paddingLeft="10dip"
            android:text="No_item_found"
            android:textColor="@color/gray_light"
            android:textSize="@dimen/text_size_normal"
            android:visibility="gone" />

        <LinearLayout
            android:id="@+id/llAddListViews"
            android:layout_width="fill_parent"
            android:layout_height="fill_parent"
            android:orientation="vertical"
            android:visibility="gone"></LinearLayout>
    </LinearLayout>

</LinearLayout>