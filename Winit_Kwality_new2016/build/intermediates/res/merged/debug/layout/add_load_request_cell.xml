<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="fill_parent"
    android:layout_height="wrap_content"
    android:background="@color/white"
    android:gravity="center_vertical"
    android:orientation="vertical" >

    <LinearLayout
        android:id="@+id/llCellClick"
        android:layout_width="fill_parent"
        android:layout_height="0dp"
        android:layout_weight="1"
        android:paddingBottom="@dimen/size5"
        android:paddingTop="@dimen/size5" >

        <ImageView
            android:id="@+id/ivAcceptCheckItems11"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:layout_marginLeft="10dip"
            android:layout_marginRight="10dip"
            android:layout_weight="0"
            android:src="@drawable/check_normal"
            android:visibility="gone" />

        <ImageView
            android:id="@+id/sep11"
            android:layout_width="wrap_content"
            android:layout_height="fill_parent"
            android:background="@drawable/sep_hor" />

        <!-- <ImageView -->
        <!-- android:layout_width="wrap_content" -->
        <!-- android:layout_height="fill_parent" -->
        <!-- android:background="@drawable/sep_hor" /> -->

        <LinearLayout
            android:layout_width="0dp"
            android:layout_height="fill_parent"
            android:layout_weight="1"
            android:gravity="center_vertical"
            android:orientation="vertical"
            android:paddingLeft="@dimen/margin_15" >

            <TextView
                android:id="@+id/tvProductKey"
                android:layout_width="@dimen/size100"
                android:layout_height="wrap_content"
                android:layout_gravity="center_vertical"
                android:gravity="center_vertical"
                android:singleLine="true"
                android:textColor="@color/greenText"
                android:textSize="@dimen/text_size_extra_large"
                android:textStyle="bold" />

            <TextView
                android:id="@+id/tvVendorName"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:maxLines="2"
                android:paddingBottom="@dimen/margin_5"
                android:textColor="@color/black"
                android:textSize="@dimen/text_size_normal"
                android:textStyle="bold" />

            <LinearLayout
                android:layout_width="fill_parent"
                android:layout_height="wrap_content" >

                <TextView
                    android:id="@+id/tvExpiryDateText"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:singleLine="true"
                    android:text="Expiry : "
                    android:textColor="@color/gray"
                    android:textSize="@dimen/text_size_small"
                    android:textStyle="bold"
                    android:visibility="visible" />

                <TextView
                    android:id="@+id/tvExpiryDate"
                    android:layout_width="@dimen/size110"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="5dip"
                    android:gravity="center_vertical"
                    android:singleLine="true"
                    android:textColor="@color/black"
                    android:textSize="@dimen/text_size_small"
                    android:textStyle="bold"
                    android:visibility="visible" />

                <TextView
                    android:id="@+id/tvItemBatchCodeText"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="@dimen/size20"
                    android:singleLine="true"
                    android:text="Batch No "
                    android:textColor="@color/gray"
                    android:textSize="@dimen/text_size_small"
                    android:textStyle="bold"
                    android:visibility="visible" />

                <TextView
                    android:id="@+id/tvItemBatchCode"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="5dip"
                    android:gravity="center_vertical"
                    android:singleLine="true"
                    android:textColor="@color/black"
                    android:textSize="@dimen/text_size_small"
                    android:textStyle="bold"
                    android:visibility="visible" />
            </LinearLayout>
        </LinearLayout>

        <LinearLayout
            android:layout_width="0dp"
            android:layout_height="fill_parent"
            android:layout_weight="1"
            android:gravity="center_vertical"
            android:orientation="horizontal" >

            <ImageView
                android:id="@+id/seperator1"
                android:layout_width="wrap_content"
                android:layout_height="fill_parent"
                android:background="@drawable/sep_hor" />

            <TextView
                android:id="@+id/tvVanRecomended"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:background="@null"
                android:gravity="center"
                android:hint="0"
                android:maxLength="5"
                android:singleLine="true"
                android:textColor="@color/gray_new"
                android:textSize="@dimen/text_size_extra_large"
                android:textStyle="bold" />

            <ImageView
                android:id="@+id/seperator2"
                android:layout_width="0.4dp"
                android:layout_height="fill_parent"
                android:background="@drawable/sep_hor" />

            <TextView
                android:id="@+id/tvVanQty"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:background="@null"
                android:gravity="center"
                android:singleLine="true"
                android:textColor="@color/gray_new"
                android:textSize="@dimen/text_size_extra_large"
                android:textStyle="bold" />

            <ImageView
                android:id="@+id/seperator3"
                android:layout_width="wrap_content"
                android:layout_height="fill_parent"
                android:background="@drawable/sep_hor"
                android:visibility="gone" />

            <EditText
                android:id="@+id/etUOM"
                android:layout_width="@dimen/size50"
                android:layout_height="fill_parent"
                android:background="@null"
                android:gravity="center"
                android:hint="0"
                android:imeOptions="actionNext"
                android:inputType="numberDecimal"
                android:maxLength="5"
                android:nextFocusDown="@+id/etInvoice2"
                android:singleLine="true"
                android:textColor="@color/gray_new"
                android:textSize="@dimen/text_size_extra_large"
                android:textStyle="bold" />

            <ImageView
                android:id="@+id/ivTotalUN"
                android:layout_width="wrap_content"
                android:layout_height="fill_parent"
                android:background="@drawable/sep_hor"
                android:visibility="gone" />

            <EditText
                android:id="@+id/etTotalQtUn"
                android:layout_width="@dimen/size80"
                android:layout_height="fill_parent"
                android:layout_marginLeft="1dip"
                android:background="@null"
                android:ellipsize="start"
                android:gravity="center"
                android:hint="0"
                android:imeOptions="actionNext"
                android:inputType="number"
                android:maxLength="5"
                android:nextFocusDown="@+id/etInvoice2"
                android:singleLine="true"
                android:textColor="@color/gray_new"
                android:textSize="@dimen/text_size_extra_large"
                android:textStyle="bold"
                android:visibility="gone" />

            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="fill_parent"
                android:background="@drawable/sep_hor" />

            <com.winit.alseer.salesman.common.CustomEditText
                android:id="@+id/etQtInCs"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                
                android:layout_weight="1"
                android:background="@null"
                android:drawableBottom="@drawable/edt_input"
                
                android:ellipsize="end"
                android:gravity="center"
                android:hint="0"
                android:imeOptions="actionNext"
                android:inputType="@null"
                android:maxLength="5"
                android:singleLine="true"
                android:textColor="@color/gray_new"
                android:textSize="@dimen/text_size_extra_large"
                android:textStyle="bold" />

            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="fill_parent"
                android:background="@drawable/sep_hor"
                android:visibility="gone" />

            <com.winit.alseer.salesman.common.CustomEditText
                android:id="@+id/etQt"
                android:layout_width="@dimen/size40"
                android:layout_height="fill_parent"
                android:layout_marginLeft="@dimen/margin_5"
                android:layout_marginRight="@dimen/margin_5"
                android:layout_weight="0"
                android:background="@null"
                android:drawableBottom="@drawable/edt_input"
                android:drawablePadding="-15dp"
                android:ellipsize="end"
                android:gravity="center"
                android:hint="0"
                android:imeOptions="actionNext"
                android:inputType="@null"
                android:maxLength="5"
                android:singleLine="true"
                android:textColor="@color/gray_new"
                android:textSize="@dimen/text_size_extra_large"
                android:textStyle="bold"
                android:visibility="gone" />

            <EditText
                android:id="@+id/etQtView"
                android:layout_width="@dimen/size70"
                android:layout_height="fill_parent"
                android:layout_marginLeft="@dimen/margin_2"
                android:layout_marginRight="@dimen/margin_2"
                android:layout_weight="0"
                android:background="@null"
                android:digits="1234567890"
                android:drawablePadding="-15dp"
                android:ellipsize="end"
                android:gravity="center"
                android:hint="0"
                android:imeOptions="actionNext"
                android:inputType="number|phone"
                android:maxLength="5"
                android:singleLine="true"
                android:textColor="@color/gray_new"
                android:textSize="@dimen/text_size_extra_large"
                android:textStyle="bold"
                android:visibility="gone" />

            <ImageView
                android:id="@+id/ivTotal"
                android:layout_width="wrap_content"
                android:layout_height="fill_parent"
                android:background="@drawable/sep_hor"
                android:visibility="gone" />

            <EditText
                android:id="@+id/etTotalQt"
                android:layout_width="@dimen/size80"
                android:layout_height="fill_parent"
                android:layout_marginLeft="1dip"
                android:background="@null"
                android:ellipsize="start"
                android:gravity="center"
                android:hint="0"
                android:imeOptions="actionNext"
                android:inputType="number"
                android:maxLength="5"
                android:nextFocusDown="@+id/etInvoice2"
                android:singleLine="true"
                android:textColor="@color/gray_new"
                android:textSize="@dimen/text_size_extra_large"
                android:textStyle="bold"
                android:visibility="gone" />

            <ImageView
                android:id="@+id/ivShippedQty"
                android:layout_width="wrap_content"
                android:layout_height="fill_parent"
                android:background="@drawable/sep_hor"
                android:visibility="gone" />

            <EditText
                android:id="@+id/etShippedQty"
                android:layout_width="@dimen/size80"
                android:layout_height="fill_parent"
                android:layout_marginLeft="1dip"
                android:background="@null"
                android:ellipsize="start"
                android:gravity="center"
                android:hint="0"
                android:imeOptions="actionNext"
                android:inputType="number"
                android:maxLength="5"
                android:nextFocusDown="@+id/etInvoice2"
                android:singleLine="true"
                android:textColor="@color/gray_new"
                android:textSize="@dimen/text_size_extra_large"
                android:textStyle="bold"
                android:visibility="gone" />

            <TextView
                android:id="@+id/etInvoice1"
                android:layout_width="60dip"
                android:layout_height="fill_parent"
                android:background="@drawable/unit_bg"
                android:gravity="center"
                android:hint="00"
                android:imeOptions="actionNext"
                android:inputType="number"
                android:maxLength="5"
                android:nextFocusDown="@+id/etInvoice2"
                android:singleLine="true"
                android:textColor="@color/gray_new"
                android:textSize="@dimen/text_size_extra_large"
                android:textStyle="bold"
                android:visibility="gone" />

            <ImageView
                android:id="@+id/ivAcceptCheckItems"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:layout_marginLeft="10dip"
                android:layout_marginRight="10dip"
                android:layout_weight="0"
                android:src="@drawable/check_normal"
                android:visibility="gone" />
        </LinearLayout>
    </LinearLayout>

    <ImageView
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:background="@drawable/sep_ver" />

    <!--  -->

</LinearLayout>